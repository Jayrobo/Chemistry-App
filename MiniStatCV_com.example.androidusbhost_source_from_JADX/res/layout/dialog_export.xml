<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <EditText android:id="@+id/exportTitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:imeOptions="actionGo|actionSearch|actionSend|actionNext|actionDone|actionPrevious" />
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <Button android:id="@+id/exportconfirm" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/expconfirm" android:layout_weight="1.0" />
        <Button android:id="@+id/exportcancel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/expcancel" android:layout_weight="1.0" android:onClick="onClick_csv" />
    </LinearLayout>
</LinearLayout>
